  <select id="selectTopTen" resultType="com.casic.oarp.datavisual.po.EvalManagementVO">
    SELECT pp3.score,pcd2.`name` comname,pcd3.`name` erjidanwei FROM
    (SELECT
    fem.score,
    SUBSTR(pcd.linkcode, 1, 12) com,
    SUBSTR(pcd.linkcode, 1, 8) erjicom
    FROM
    pub_corp_dept pcd,
    fx_eval_management fem
    WHERE
    pcd.id = fem.corpid
    ORDER BY
    fem.score DESC
    LIMIT 10) pp3
    LEFT JOIN pub_corp_dept pcd2 ON pp3.com = pcd2.linkcode
    LEFT JOIN pub_corp_dept pcd3 on pp3.erjicom = pcd3.linkcode
    ORDER BY
    pp3.score DESC
  </select>
  <select id="selectAllCom" resultType="com.casic.oarp.datavisual.po.EvalManagementVO">
    SELECT pp3.score,pcd2.`name` comname,pcd3.`name` erjidanwei FROM
    (SELECT
    fem.score,
    SUBSTR(pcd.linkcode, 1, 12) com,
    SUBSTR(pcd.linkcode, 1, 8) erjicom
    FROM
    pub_corp_dept pcd,
    fx_eval_management fem
    WHERE
    pcd.corptype in ('二级机构部门') AND
    pcd.id = fem.corpid) pp3
    LEFT JOIN pub_corp_dept pcd2 ON pp3.com = pcd2.linkcode
    LEFT JOIN pub_corp_dept pcd3 on pp3.erjicom = pcd3.linkcode
    ORDER BY
    pp3.score DESC
  </select>
